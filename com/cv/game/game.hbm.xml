<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cv.game">
    <class name="GameData" table="GAME" catalog="root">
        <id name="gameId" type="java.lang.Long">
            <column name="GAME_ID" />
            <generator class="identity" />
        </id>
        <property name="startTime" type="java.lang.Long">
            <column name="START_TIME" />
        </property>
        <property name="endTime" type="java.lang.Long">
            <column name="END_TIME" />
        </property>
        <property name="team1" type="java.lang.Long">
            <column name="TEAM_1" />
        </property>
        <property name="team2" type="java.lang.Long">
            <column name="TEAM_2" />
        </property>
    </class>
</hibernate-mapping>
